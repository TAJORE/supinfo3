<div id="bodyEmoticon" style="display: none">
    <div class="row justify-content-center bodyEmoticon">
        <div class="col-5 list">
            <div class="list-group">
                <a href="#" class="list-group-item active small">
                    Cras justo
                </a>
                {% for i in 1..55 %}
                    <a href="#" class="list-group-item list-group-item-action small">liste des emoticons {{ i }} </a>
                {% endfor %}
            </div>
        </div>
        <div class="col emoji">
            <div class="row">
                {% for i in 1..40 %}
                    <div class="col-3 element">
                        <img src="{{ asset('data/img/empty.png') }}" alt="" style="margin-bottom: 3%; height: 40px; width: 40px;">
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>